---
export interface Props {
  emContent: string;
  title: string;
}

const { emContent, title } = Astro.props;
---
<section id="form-section" class="py-5 bg-light text-center p-5">
  <div class="form-container">
    <h1 class="section-title mb-4 p-4"><em>{emContent}</em>{title}</h1>
    <script src="https://popstar-photo-booths.checkcherry.com/api/checkcherry_widgets" type="text/javascript" charset="utf-8"></script>
    <div class="checkcherry__widget__contact-form" data-props='{"apiKey":"TCN-R4JQ-W4A","contactFormId":10776,"iframe":false,"host":"https://popstar-photo-booths.checkcherry.com"}'></div>
  </div>
</section>

